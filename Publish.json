{
  "chef_namespace": "Chef.Bootstrap.WindowsAzure.Test",

  "isNewExtension": "true",

  "prod": {
    "publishSettingsFile": "change_me.publishsettings",
    "subscriptionName": "change_me_subscription_name",
    "publishUri": "https://management.core.windows.net/<change_me_subscription_id>/services/extensions"
  },

  "preview": {
    "publishSettingsFile": "change_me.publishsettings",
    "subscriptionName": "change_me_subscription_name",
    "publishUri": "https://management-preview.core.windows-int.net/<change_me_subscription_id>/services/extensions"
  },

  "windows": {
    "definitionParams": {
      "isInternal": "true",
      "extensionName": "ChefClient",
      "storageAccount": "chefextensions",
      "storageContainer": "windows"
    }
  },
  "ubuntu": {
    "definitionParams": {
      "isInternal": "true",
      "extensionName": "LinuxChefClient",
      "storageAccount": "chefextensions",
      "storageContainer": "ubuntu"
    }
  },
  "centos": {
    "definitionParams": {
      "isInternal": "true",
      "extensionName": "CentosChefClient",
      "storageAccount": "chefextensions",
      "storageContainer": "centos"
    }
  }
}